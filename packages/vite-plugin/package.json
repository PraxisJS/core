{
  "name": "@oxog/praxis-vite-plugin",
  "version": "1.0.0",
  "description": "Vite plugin for Praxis framework - optimized builds and HMR support",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest"
  },
  "keywords": [
    "praxis",
    "vite",
    "plugin",
    "hmr",
    "build"
  ],
  "author": "PraxisJS Team <team@praxisjs.com>",
  "license": "MIT",
  "homepage": "https://praxisjs.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/praxisjs/core.git",
    "directory": "packages/vite-plugin"
  },
  "peerDependencies": {
    "@oxog/praxis": "^1.0.0",
    "vite": "^4.0.0 || ^5.0.0"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vite": "^5.0.0",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
